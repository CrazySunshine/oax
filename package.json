  "private": true,
  "scripts": {
    "app:http-data": "node build/app/http-data.js header > src/assets/http-header.json && node build/app/http-data.js method > src/assets/http-method.json && node build/app/http-data.js status > src/assets/http-status.json",
    "bundle": "node build/schema-bundle.js",
    "build": "node build/build.js",
    "deploy": "npm --no-git-tag-version version patch && node build/build.js && deploy-to-gh-pages --local ./dist",
    "deploy-404": "npm --no-git-tag-version version patch && node build/build.js && node ./build/app/copy.js dist/index.html dist/404.html && deploy-to-gh-pages --local ./dist",
    "deploy-push": "cd ./dist && git push --force \"https://github.com/darosh/oax.git\" master:gh-pages",
    "dev": "node build/dev-server.js",
    "doc:screenshots": "node ./build/doc/screenshots.js && node ./build/doc/screenshots-index.js",
    "doc:screenshots-xs": "node ./build/doc/screenshots.js xs && node ./build/doc/screenshots-index.js xs",
    "doc:screenshots-xs+md": "node ./build/doc/screenshots.js xs,md && node ./build/doc/screenshots-index.js xs,md",
    "doc:test-links": "node ./build/doc/test-links.js",
    "imagemin": "imagemin doc/screenshots/images/*.png --out-dir doc/screenshots/images",
    "lint": "eslint --ext .js,.vue src",
    "report": "npm run build --report",
    "tslint": "tslint --fix -c tslint.json src/**/*.ts",
    "vuetify": "node ./build/app/copy.js ./config/vuetify/VIcon.vue ./node_modules/vuetify/src/components/VIcon/VIcon.vue && node ./build/app/copy.js ./config/vuetify/_custom.styl ./node_modules/vuetify/src/stylus/settings/_custom.styl"
  }
}
