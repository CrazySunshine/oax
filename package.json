  "scripts": {
    "data:bundle": "node build/schema-bundle.js",
    "data:dependencies": "node ./build/app/dependencies.js",
    "data:http": "node build/app/http-data.js header > assets/data/http-header.json && node build/app/http-data.js method > assets/data/http-method.json && node build/app/http-data.js status > assets/data/http-status.json",
    "doc:imagemin": "imagemin doc/screenshots/images/*.png --out-dir doc/screenshots/images",
    "doc:screenshots": "node ./build/doc/screenshots.js && node ./build/doc/screenshots-index.js",
    "doc:screenshots-xs": "node ./build/doc/screenshots.js xs && node ./build/doc/screenshots-index.js xs",
    "doc:screenshots-xs+md": "node ./build/doc/screenshots.js xs,md && node ./build/doc/screenshots-index.js xs,md",
    "doc:test-links": "node ./build/doc/test-links.js",
    "lint:js": "eslint --ext .js,.vue assets components pages store plugins",
    "lint:ts": "tslint --fix -c tslint.json assets/**/*.ts components/**/*.ts",
    "nuxt:build": "nuxt build",
    "nuxt:dev": "nuxt",
    "nuxt:generate": "nuxt generate",
    "nuxt:start": "nuxt start",
    "vendor:vuetify": "cross-env NODE_ENV=es5 babel config/vuetify/src --out-dir config/vuetify/es5 && node ./build/app/patch.js vuetify",
    "vendor:walk-parse5": "cross-env NODE_ENV=es5 babel node_modules/walk-parse5 --out-dir node_modules/walk-parse5",
    "webpack:build": "node build/build.js",
    "webpack:deploy": "npm --no-git-tag-version version patch && npm run data:dependencies && node build/build.js && deploy-to-gh-pages --local ./dist",
    "webpack:dev": "node build/dev-server.js",
    "webpack:report": "npm run webpack:build --report",
    "postinstall": "npm run vendor:vuetify && npm run vendor:walk-parse5"
  }
